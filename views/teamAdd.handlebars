<form action="/teamAdd/"  method="post" enctype="multipart/form-data">
    <div>
        <label for="">ID</label>
        <div>
            <input type="text" name="id" placeholder="555">
        </div>
    </div>

    <div>
        <label for="">Club Name</label>
        <div>
            <input type="text" name="clubName" placeholder="Arsenal FC">
        </div>
    </div>

    <div>
        <label for="">Shortname</label>
        <div>
            <input type="text" name="shortName" placeholder="Arsenal">
        </div>
    </div>

    <div>
        <label for="">TLA</label>
        <div>
            <input type="text" name="tla" placeholder="ARS">
        </div>
    </div>

    <div>
        <label for="">Country</label>
        <div>
            <input type="text" name="country" placeholder="England">
        </div>
    </div>

    <div>
        <label for="">Foundation</label>
        <div>
            <input type="text" name="foundation" placeholder="1886">
        </div>

    </div>

    <div>
        <label for="">Venue</label>
        <div>
            <input type="text" name="venue" placeholder="Emirates Stadium">
        </div>
        
    </div>

    <div>
        <label for="">Address</label>
        <div>
            <input type="text" name="address" placeholder="75 Drayton Park London N5 1BU">
        </div>
    </div>

    <div>
        <label for="">Website</label>
        <div>
            <input type="text" name="website" placeholder="http://arsenal.com">
        </div>
    </div>
    
    <div>
        <label for="">Email</label>
        <div>
            <input type="text" name="email" placeholder="info@arsenal.co.uk">
        </div>
    </div>

    <div>
        <label for="">Club Crest</label>
        <div>
            <input type="file" name="clubCrest" placeholder="Club Crest">
        </div>
    </div>

    <button type="submit">Submit</button>
</form>